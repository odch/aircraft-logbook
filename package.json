{
  "name": "aircraft-logbook",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/odch/aircraft-logbook.git",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config ./webpack.config.js --progress --profile --colors",
    "codecov": "codecov",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:test": "npm run start:cy & npm run cy:run",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "start:cy": "npm run start --project=cypress",
    "start:dev": "npm run start --project=dev",
    "start:prod": "npm run start --project=prod",
    "test": "TZ=UTC jest"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!/node_modules/**",
      "!src/index.js"
    ],
    "setupFilesAfterEnv": [
      "./src/testutil/setupTests.js"
    ],
    "globals": {
      "__CONF__": {}
    },
    "moduleNameMapper": {
      ".+\\.(svg|png|jpg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "codecov": "^3.0.4",
    "copy-webpack-plugin": "^5.1.2",
    "cypress": "^5.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-cypress": "^2.6.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "fast-csv": "^4.3.2",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.4.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "prettier": "^2.1.1",
    "pretty": "^2.0.0",
    "react-hot-loader": "^4.3.5",
    "react-test-renderer": "^16.9.0",
    "redux-mock-store": "^1.5.3",
    "redux-saga-test-plan": "^4.0.0-beta.4",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "@date-io/core": "^1.3.8",
    "@date-io/moment": "^1.1.0",
    "@material-ui/core": "^4.3.3",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/pickers": "^3.2.0",
    "feature-toggles": "^1.4.0",
    "firebase": "^6.3.3",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "mobile-detect": "^1.4.4",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "prop-types": "^15.6.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-intl": "^5.8.0",
    "react-intl-polyfill": "^1.0.3",
    "react-redux": "^7.1.0",
    "react-redux-firebase": "^2.1.8",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.0.4",
    "redux": "^4.0.0",
    "redux-firestore": "^0.5.8",
    "redux-saga": "^1.0.5"
  }
}
