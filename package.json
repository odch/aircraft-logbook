{
  "name": "aircraft-logbook",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/odch/aircraft-logbook.git",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config ./webpack.config.js --progress --profile --colors",
    "codecov": "codecov",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:test": "npm run start:cy & npm run cy:run",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "start:cy": "npm run start --project=cypress",
    "start:dev": "npm run start --project=dev",
    "start:prod": "npm run start --project=prod",
    "test": "jest"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!/node_modules/**",
      "!src/index.js"
    ],
    "setupTestFrameworkScriptFile": "./src/testutil/setupTests.js",
    "globals": {
      "__CONF__": {}
    }
  },
  "devDependencies": {
    "@firebase/functions": "^0.3.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "codecov": "^3.0.4",
    "cypress": "^3.1.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "prettier": "1.14.2",
    "pretty": "^2.0.0",
    "react-hot-loader": "^4.3.5",
    "react-test-renderer": "^16.4.2",
    "redux-mock-store": "^1.5.3",
    "redux-saga-test-plan": "^3.7.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "@date-io/moment": "^0.0.2",
    "@firebase/app": "^0.3.4",
    "@firebase/auth": "^0.7.8",
    "@firebase/firestore": "^0.8.6",
    "@material-ui/core": "^3.1.0",
    "@material-ui/icons": "^3.0.1",
    "feature-toggles": "^1.4.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "material-ui-pickers": "^2.0.3",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-intl": "^2.7.0",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.1.8",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.1.2",
    "redux": "^4.0.0",
    "redux-firestore": "^0.5.8",
    "redux-saga": "^0.16.0"
  }
